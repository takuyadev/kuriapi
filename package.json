{
   "scripts": {
      "status": "tsc --p ./tsconfig.json --listFiles",
      "test": "jest",
      "start": "node ./dist/server.js",
      "build": "tsc",
      "seed": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/db/seed.ts",
      "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register src/server.ts"
   },
   "_moduleAliases": {
      "@/routes": "dist/routes",
      "@/data": "dist/data",
      "@/middlewares": "dist/middlewares",
      "@/controllers": "dist/controllers",
      "@/db": "dist/db",
      "@/lib": "dist/lib",
      "@/utils": "dist/utils"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "helmet": "^6.0.1",
      "hpp": "^0.2.3",
      "pg": "^8.10.0",
      "typescript": "^5.0.4",
      "xss-clean": "^0.1.1"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cors": "^2.8.13",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.17",
      "@types/hpp": "^0.2.2",
      "@types/jest": "^29.5.1",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/morgan": "^1.9.4",
      "@types/node": "^18.13.0",
      "@types/pg": "^8.6.6",
      "jest": "^29.5.0",
      "module-alias": "^2.2.2",
      "morgan": "^1.10.0",
      "nodemon": "^2.0.22",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0"
   }
}
